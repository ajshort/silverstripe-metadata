		"email": "marcus@symbiote.com.au"
	}],
	"require": {
		"silverstripe/framework": "~3.1",
        "mustangostang/spyc": "^0.6.2",
		"silverstripe/cms": "~3.1",
		"symbiote/silverstripe-gridfieldextensions": "~1.0"
	},
	"extra": {
		"installer-name": "metadata",
		"branch-alias": {
			"dev-master": "4.0.x-dev"
